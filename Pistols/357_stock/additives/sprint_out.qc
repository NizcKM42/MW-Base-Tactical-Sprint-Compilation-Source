//Weights


// To Sub
$animation sprint_out_to_sub "anims\vm_pi_cpapa_super_sprint_to_walk.smd"

// Delta
$sequence "a_sprint_to_walk" {
	"anims\vm_pi_cpapa_super_sprint_to_walk.smd"
	subtract sprint_out_to_sub 24
	delta
}

// Sequence
$sequence "sprint_out" {
	"anims\vm_pi_cpapa_stock_idle.smd" //this is one frame so fps 1 is 1 second
	fps 1.25 //30 divided by the number of frames of a_jog_to_walk
	addlayer "a_sprint_to_walk"
	addlayer "jog_loop"
	addlayer "walk_loop"
	ikrule rhand touch "tag_pistol_offset" usesource
    ikrule lhand touch "tag_pistol_offset" usesource
}