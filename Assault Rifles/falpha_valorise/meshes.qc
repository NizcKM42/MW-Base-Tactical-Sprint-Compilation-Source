//MESHES

$maxverts 9999999
$illumposition 0 0 0

$origin 0 0 0 -90

$bodygroup "receiver"
{
	studio "meshes/attachment_vm_ar_falpha_receiver.smd"
}

$bodygroup "sight"
{
	studio "meshes/attachment_vm_ar_falpha_sight_on.smd"
	studio "meshes/attachment_vm_ar_falpha_sight_off.smd"
	blank
}

$bodygroup "tag_stock"
{
	studio "meshes/attachment_vm_ar_falpha_stock_attach.smd"
	blank
}

$bodygroup "tag_toprail"
{
	studio "meshes/attachment_vm_ar_falpha_toprail.smd"
}

$bodygroup "tag_grip_hide"
{
	blank
	studio "meshes/attachment_vm_ar_falpha_grip_hide.smd"
}


$scale 0.3937
$bodygroup "tag_pgrip"
{
	studio "meshes/attachment_vm_ar_falpha_triggrip.smd"
}

$bodygroup "tag_laser_mount"
{	
	blank
	studio "meshes/attachment_vm_ar_falpha_laser_mount.smd"
}